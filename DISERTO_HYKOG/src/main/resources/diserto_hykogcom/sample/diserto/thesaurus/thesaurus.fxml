<StackPane fx:id="stackPane" maxHeight="300.0" maxWidth="443.0" prefHeight="260.0" prefWidth="443.0" style="-fx-border-color: D3D3D3; -fx-background-color: D3D3D3;             -fx-background-radius: 15px; -fx-border-radius: 15px;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">

    <children>
        <VBox fx:id="vbox">
            <children>
                <ImageView fitHeight="172.0" fitWidth="441.0" pickOnBounds="true" preserveRatio="true" smooth="false">

                </ImageView>
                <Label prefHeight="27.0" prefWidth="242.0">
                    <padding>
                        <Insets left="20.0" />
                    </padding>
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <HBox spacing="32.5">
                    <children>

                        <TextField fx:id="textFieldPath" maxHeight="22.0" maxWidth="300.0" minHeight="22.0" minWidth="300.0" prefHeight="22.0" prefWidth="300.0">
                            <VBox.margin>
                                <Insets bottom="10.0" top="5.0" />
                            </VBox.margin>
                            <font>
                                <Font size="13.0" />
                            </font>
                        </TextField>
                        <Button fx:id="browse" alignment="TOP_CENTER" cancelButton="true" contentDisplay="CENTER" maxHeight="18.0" maxWidth="68.0" minHeight="22.0" minWidth="68.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="67.0" text="Browse" textAlignment="CENTER" wrapText="true">
                            <font>
                                <Font size="11.0" />
                            </font></Button>
                    </children>
                    <padding>
                        <Insets bottom="8.0" left="20.0" />
                    </padding>
                </HBox>
                <HBox alignment="BOTTOM_RIGHT" spacing="20.0">
                    <children>
                        <Button fx:id="previous" alignment="CENTER" cancelButton="true" maxHeight="22.0" maxWidth="68.0" minHeight="22.0" minWidth="68.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="68.0" text="Previous" textAlignment="CENTER" wrapText="true">
                            <VBox.margin>
                                <Insets bottom="10.0" top="5.0" />
                            </VBox.margin>
                            <font>
                                <Font size="11.0" />
                            </font>
                        </Button>
                        <Button fx:id="next" alignment="CENTER" cancelButton="true" maxHeight="42.0" minHeight="18.0" minWidth="68.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="68.0" text="Next" textAlignment="CENTER" wrapText="true">
                            <VBox.margin>
                                <Insets bottom="10.0" top="5.0" />
                            </VBox.margin>
                            <font>
                                <Font size="11.0" />
                            </font>
                        </Button>
                    </children>
                    <padding>
                        <Insets bottom="5.0" right="20.0" />
                    </padding>
                </HBox>
            </children>
        </VBox>
    </children>
</StackPane>
